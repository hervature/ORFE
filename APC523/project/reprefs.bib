@article{Liu2016,
abstract = {In this paper, a stochastic linear theta (SLT) method is introduced and analyzed for neutral stochastic differential delay equations (NSDDEs). We give some conditions on neutral item, drift and diffusion coefficients, which admit that the diffusion coefficient can be highly nonlinear and does not necessarily satisfy a linear growth or global Lipschitz condition. It is proved that, for all positive stepsizes, the SLT method with $\theta$ ∈ [1/2, 1] is asymptotically mean stable and so is $\theta$ ∈ [0, 1/2) under a stronger assumption. Furthermore, we consider the split-step theta (SST) method and obtain a similar but better result. That is, the SST method with $\theta$ ∈ [1/2, 1] is exponentially mean stable and so is $\theta$ ∈ [0, 1/2). Finally, two numerical examples are given to show the efficiency of the obtained results.},
author = {Liu, Linna and Zhu, Quanxin},
doi = {10.1016/j.cam.2016.03.021},
file = {:home/gerry/Dropbox/APC523/Project/texts+notes/ms{\_}stability{\_}SDE{\_}paper.pdf:pdf},
issn = {03770427},
journal = {Journal of Computational and Applied Mathematics},
keywords = {Exponential stability,Mean square stability,Neutral stochastic differential delay equation,Split-step theta method,Stochastic linear theta method},
mendeley-groups = {SDE{\_}project},
pages = {55--67},
publisher = {Elsevier B.V.},
title = {{Mean square stability of two classes of theta method for neutral stochastic differential delay equations}},
url = {http://dx.doi.org/10.1016/j.cam.2016.03.021},
volume = {305},
year = {2016}
}


