pages = {361},
publisher = {Springer},
title = {{Statistical Methods in Quantum Optics 1: Master Equations and Fokker-Planck Equations}},
url = {http://books.google.ca/books/about/Statistical{\_}Methods{\_}in{\_}Quantum{\_}Optics{\_}1.html?id=ocgRgM-yJacC{\&}pgis=1},
year = {1999}
}


